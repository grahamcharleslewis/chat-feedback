<div id="<%= dom_id feedback %>">
  <% unless feedback.chat.nil? %>
    <p>
      <strong>Chat:</strong>
      <%= feedback.chat.id %>
    </p>
  <% end %>

  <p>
    <strong>Uuid:</strong>
    <%= feedback.uuid %>
  </p>

  <p>
    <strong>Version:</strong>
    <%= ENV["#{feedback.level.upcase}_FEEDBACK_VERSION"] %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= feedback.level %>
  </p>

  <p>
    <strong>Response:</strong>
    <%= feedback.response %>
  </p>
</div>
