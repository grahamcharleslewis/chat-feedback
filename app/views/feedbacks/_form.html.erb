<%= form_with(model: feedback) do |form| %>
  <% if feedback.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
        <% feedback.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :chat_id, style: "display: block" %>
    <%= form.number_field :chat_id %>
  </div>

  <div>
    <%= form.label :uuid, style: "display: block" %>
    <%= form.text_field :uuid %>
  </div>

  <div>
    <%= form.label :version, style: "display: block" %>
    <%= form.text_field :version %>
  </div>

  <div>
    <%= form.label :level, style: "display: block" %>
    <%= form.text_field :level %>
  </div>

  <div>
    <%= form.label :response, style: "display: block" %>
    <%= form.text_field :response %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
